<?xml version="1.0" encoding="UTF-8"?>
<AMConfig>
	<CommonConfig>
		<IndexBase>D:/tmp</IndexBase><!-- 索引根目录 -->
	</CommonConfig>
	<Indexers><!-- 索引器配置，支持多个索引器，每个索引器对应一个索引文件，增加索引器只需增加一个&lt;entry&gt;项即可 -->
		<entry>
			<IndexerName>poi1</IndexerName><!--1 lcd-->
			<IndexerConfig><!-- 索引器配置 -->
				<IndexName>poi1</IndexName>
				<AnalyzerClass>com.chenlb.mmseg4j.analysis.ComplexAnalyzer</AnalyzerClass>
				<BreakPoint>d:/tmp/poi1.bp</BreakPoint>
				<DataSource></DataSource>
				<FullSelectSql>select t.id pid, t.mc addr,t.ssqx code,t.point_x pos_x,t.point_y pos_y from poi1 t where t.ssqx is not null</FullSelectSql>
				<IncrementSelectSql></IncrementSelectSql>
				<PKFieldName>pid</PKFieldName>
				<IndexFiledName>addr</IndexFiledName>
				<DataFiledName>pos_x,pos_y,code</DataFiledName>
			</IndexerConfig>
		</entry>
